{
  "schemaVersion": 1,
  "id": "mcskyblock",
  "version": "${version}",

  "name": "MC Skyblock Fabric",
  "description": "This mod adds a new world generation type for a barebones skyblock experience (Fabric Port)",
  "authors": [
    "Ant",
    "m_jimmer"
  ],
  "contact": {
    "homepage": "https://github.com/AntMCDev/MCSkyblockFabric",
    "sources": "https://github.com/AntMCDev/MCSkyblockFabric"
  },

  "license": "MIT",
  "icon": "assets/mcskyblock/icon.png",
  "accessWidener": "mcskyblock.accesswidener",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.ant.mcskyblock.FabricMCSkyBlock"
    ],
    "fabric-datagen": [
      "com.ant.mcskyblock.fabric._build.SkyblockDataGenerator"
    ],
    "modmenu": [
      "com.ant.mcskyblock.fabric.config.SkyBlockModMenu"
    ]
  },
  "mixins": [
    "mcskyblock.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
